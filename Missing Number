//https://leetcode.com/problems/missing-number/
Without Bit manipulation:
class Solution {
public:
    int findMissing(vector<int>& num)
    {
        sort(num.begin(),num.end());
        int m=num.size();
        
        vector<int> value(m+1);
        for(int i=0;i<m+1;i++)
        {
            value[i]=i;
          
        }
        int res=value[value.size()-1];
        for(int i=0;i<m;i++)
        {
            cout<<value[i]<<" "<<num[i]<<endl;
            if(num[i]!=value[i]){
            res=value[i];
            break;
            }
        }
        return res;  
    }
    int missingNumber(vector<int>& nums) {
        int result=findMissing(nums);
        return result;
    }
};
With bit manipulation:
class Solution {
public:
    int findMissing(vector<int>& num)
    {
        sort(num.begin(),num.end());
        int m=num.size();
        int res=0;
        int xnum=0;
        int diff=0;
        for(int i=0;i<m+1;i++)
        {
           res=res^i;
          
        }
        cout<<res<<endl;
        for(int i=0;i<m;i++)
        {
           xnum=xnum^num[i]; 
        }
      
        diff=res^xnum;
        return diff;  
    }
    int missingNumber(vector<int>& nums) {
        int result=findMissing(nums);
        return result;
    }
};
